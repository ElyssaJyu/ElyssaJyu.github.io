{
    "lang": "en-us",
    "name": " My Browsing Portrait",
    "short_name": "Browser Portrait",
    "description": "Analyze user's browsing history and generate a portrait of the user.",
    "start_url": "./",
    "scope": "./",
    "background_color": "#2f3d58",
    "theme_color": "#2f3d58",
    "orientation": "portrait",
    "display": "standalone",
    "display_override": [
        "window-controls-overlay"
    ],
    "icons": [
        {
            "src": "/hand.png",
            "sizes": "45x45"
        }
    ],
    "share_target": {
        "action": "",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "mapped_title",
            "text": "mapped_text",
            "url": "mapped_url",
            "files": [
                {
                    "name": "mapped_files",
                    "accept": [
                        "image/jpeg"
                    ]
                }
            ]
        }
    },
    "edge_side_panel": {
        "preferred_width": 480
    }
}